#. Piecewise.xml:89(glossterm) Piecewise.xml:113(glossterm) Piecewise.xml:138(glossterm) Piecewise.xml:163(glossterm) Errors.xml:16(glossterm) Errors.xml:102(glossterm) Special.xml:37(glossterm) Special.xml:226(glossterm) Special.xml:336(glossterm) Special.xml:370(glossterm) OptionsDialog.xml:116(glossterm)
msgid "real numbers"
