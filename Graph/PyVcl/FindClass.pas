function LookUpClass(Name : string) : PTypeInfo;
var
  I : Integer;
begin
  if TypeList.Find(Name, I) then
    Result := PTypeInfo(TypeList.Objects[I])
  else
    Result := nil;
end;

function GetTypeList : TStringList;
begin
  Result := TypeList;
end;

