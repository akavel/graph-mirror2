#. Special.xml:32(glossterm) Special.xml:61(glossterm) Special.xml:87(glossterm) Special.xml:160(glossterm) Special.xml:244(glossterm) Special.xml:267(glossterm) Piecewise.xml:70(glossterm) Piecewise.xml:94(glossterm) Piecewise.xml:119(glossterm) Piecewise.xml:142(glossterm) Piecewise.xml:167(glossterm)
msgid "numeric expressions"
